<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	@Styles.Render("~/Content/bootstrap.min.css")
	@Styles.Render("~/Content/bootstrap-grid.min.css")
	<title>@ViewBag.Title</title>
</head>
<body>
	@Scripts.Render("~/Scripts/jquery-3.0.0.min.js")
	@Scripts.Render("~/Scripts/bootstrap.min.js")
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
		<div class="collapse navbar-collapse" id="navbarNav">
			<ul class="navbar-nav">
				<li class="nav-item active">
					@Html.ActionLink("Главная", "Index", "Home", null, new { @class = "nav-link active" })
				</li>
				<li class="nav-item">
					@Html.ActionLink("Добавить кандидата", "AddCandidateForm", "Home", null, new { @class = "nav-link" })
				</li>
				<li class="nav-item">
					@Html.ActionLink("Тесты", "Index", "Test", null, new { @class = "nav-link" })
				</li>
				<li class="nav-item">
					@Html.ActionLink("Оценка эффективности", "Efficiency", "Candidate", null, new { @class = "nav-link" })
				</li>
				<li class="nav-item">
					@Html.ActionLink("Зарегистрировать пользователя", "AddNewUser", "Home", null, new { @class = "nav-link" })
				</li>
			</ul>
		</div>
	</nav>
	@RenderBody()
</body>
</html>
